{
  "name": "agentlink",
  "version": "1.0.0",
  "private": true,
  "description": "AgentLink - 跨域存储服务",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/server",
    "build": "npm run build --workspaces",
    "build:core": "npm run build --workspace=packages/core",
    "build:sdk": "npm run build --workspace=packages/sdk",
    "build:server": "npm run build --workspace=apps/server"
  },
  "devDependencies": {
    "typescript": "^5.0.2"
  }
}

